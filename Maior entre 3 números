#include <stdio.h>
#include <stdlib.h>
int main()
{
    int x,y,z;
    printf("\tDigite 3 numeros:");
    scanf("%d%d%d",&x, &y, &z);
    if((x==y)&&(x==z)&&(y==z)&&(y==x)&&(z==x)&&(z==y)){
        printf("\n\n%d e o maior numero", x);
    }
    if(x>=y && x>z){
        printf("\n\n%d e o maior numero", x);
    }
    if(y>x && y>=z){
        printf("\n\n%d e o maior numero", y);
    }
    if(z>=x && z>y){
        printf("\n\n%d e o maior numero", z);
    }

    return 0;

}
