#include <stdio.h>
#include <stdlib.h>
int main()
{
    int x,y,z,a,b,c;
    printf("\tDigite 3 numeros:");
    scanf("%d%d%d",&x, &y, &z);
    if(x<=y){
        if(x<=z){
           a=x;
           if(y<=z){
            b=y, c=z;
            }
           else
            b=z, c=y;
        }
        else
         a=z, b=x, c=y;
        }
        else
        if(y<=z){
            a=y;
            if(x<=z){
            b=x, c=z;
        }
        else
            b=z, c=x;
            }
        else
        a=z, b=y, c=x;
        printf("\n\n%d ; %d ; %d", a, b, c);


    return 0;
}
